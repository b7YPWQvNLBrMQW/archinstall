{
    "app_config": {
        "audio_config": {
            "audio": "pulseaudio"
        },
        "bluetooth_config": {
            "enabled": true
        }
    },
    "archinstall-language": "English",
    "auth_config": {},
    "bootloader_config": {
        "bootloader": "Systemd-boot",
        "removable": false,
        "uki": true
    },
    "custom_commands": [
        "systemctl enable NetworkManager wpa_supplicant",
        "reflector --sort rate --latest 20 --protocol https --save /etc/pacman.d/mirrorlist && pacman -Syu --noconfirm",
        "export USER_NAME=$(find /home/* -maxdepth 0 -type d ! '(' -name lost+found ')' -printf '%f\n')",
        "su -c 'gsettings set org.gnome.desktop.interface color-scheme prefer-dark' $USER_NAME",
        "su -c 'gsettings set org.gnome.desktop.wm.preferences button-layout :minimize,maximize,close' $USER_NAME",
        "su -c 'for UUID in extension-list@tu.berry dash-to-panel@jderose9.github.com apps-menu@gnome-shell-extensions.gcampax.github.com; do gnome-browser-connector gnome-extensions://${UUID/@/%40}/?action=install && gnome-extensions enable $UUID; done' $USER_NAME",
        "su -c 'git clone https://aur.archlinux.org/yay.git /tmp/yay && cd /tmp/yay && makepkg -si --noconfirm && cd / && rm -rf /tmp/yay' $USER_NAME",
        "su -c 'yay -S gdm-settings python-setuptools-reproducible protonup-qt steamtinkerlaunch dxvk-bin lutris-wine-meta bottles visual-studio-code-bin' $USER_NAME",
        "su -c 'cd /home/$USER_NAME && wget https://repo.anaconda.com/miniconda/Miniconda3-latest-Linux-x86_64.sh && bash /home/$USER_NAME/Miniconda3-latest-Linux-x86_64.sh && rm -f /home/$USER_NAME/Miniconda3-latest-Linux-x86_64.sh' $USER_NAME"
    ],
    "kernels": [
        "linux",
        "linux-lts"
    ],
    "locale_config": {
        "kb_layout": "us",
        "sys_enc": "UTF-8",
        "sys_lang": "en_US.UTF-8"
    },
    "mirror_config": {
        "custom_repositories": [],
        "custom_servers": [],
        "mirror_regions": {},
        "optional_repositories": [
            "multilib"
        ]
    },
    "network_config": {
        "type": "nm"
    },
    "ntp": true,
    "packages": [
        "git","base-devel", "nano", "vim", "openssh", "htop", "wget", "iwd", "wireless_tools", "smartmontools", "xdg-utils",
        "gnome-tweaks", "gnome-shell-extensions", "gnome-browser-connector", 
        "timeshift","reflector","fastfetch","btop",
        "firefox","keepass","nextcloud-client","discord","moonlight-qt","transmission-remote-gtk","pycharm-community-edition","freecad","gimp",
        "steam","lutris","dolphin-emu","wine-staging","wine-gecko","wine-mono","winetricks","lib32-gnutls","lib32-libldap","lib32-libgpg-error","lib32-sqlite","lib32-libpulse","lib32-alsa-plugins","gamemode","lib32-gamemode","mangohud","lib32-mangohud"
    ],
    "parallel_downloads": 5,
    "profile_config": {
        "gfx_driver": "All open-source",
        "greeter": "gdm",
        "profile": {
            "custom_settings": {
                "GNOME": {}
            },
            "details": [
                "GNOME"
            ],
            "main": "Desktop"
        }
    },
    "script": null,
    "services": [],
    "swap": true,
    "timezone": "US/Eastern",
    "version": "3.0.14"
}
